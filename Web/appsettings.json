{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "Redis": {
    "RedisCachingEnabled": true,
    "RedisCachingConnectionString": "127.0.0.1:6379,ssl=False",
    "PersistDataProtectionKeysToRedis": false
  },
  "AllowedHosts": "*",
  "PluginLoader": {
    "Assemblies": [
      "Demo.Module.Notification"
    ]
  }
}
